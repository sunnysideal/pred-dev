{
  "name": "NeuralProphet Add-on",
  "version": "0.0.7",
  "slug": "neuralprophet_addon",
  "description": "A custom Home Assistant add-on",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "options": {
    "log_level": "info"
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?"
  },
  "ports": {
    "8080/tcp": 8080
  },
  "ports_description": {
    "8080/tcp": "Web interface port"
  },
  "homeassistant_api": true,
  "hassio_api": true,
  "hassio_role": "default",
  "auth_api": true,
  "ingress": true,
  "ingress_port": 8080,
  "panel_icon": "mdi:puzzle"
}
